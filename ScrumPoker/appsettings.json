{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AppSettings": {
    "Redis": {
      "AllowAdmin": true,
      "Ssl": false,
      "AbortConnect": false,
      "ConnectTimeout": 6000,
      "ConnectRetry": 3,
      "Database": 0,
      "Hosts": [
        {
          "Host": "redis",
          "Port": "6379"
        }
      ]
    }
  },
  "AllowedHosts": "*"
}
